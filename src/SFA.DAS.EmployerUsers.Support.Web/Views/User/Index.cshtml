@model SFA.DAS.EmployerUsers.Support.Core.Domain.Model.EmployerUser

<h1>Overview</h1>

@if (Model != null)
{
    <dl>

        <dt>Name:</dt>
        <dd>@Model.FirstName @Model.LastName</dd>


        <dt>Email:</dt>
        <dd><a href="mailto:@Model.Email">@Model.Email</a></dd>

        <dt>Status:</dt>
        <dd>@Model.Status</dd>

    </dl>
}
else
{
    <p>User not found</p>
}
